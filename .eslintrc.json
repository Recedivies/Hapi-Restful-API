{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "eol-last": "off",
        "indent": "off",
        "no-trailing-spaces": "off",
        "object-curly-spacing": "off",
        "no-multiple-empty-lines": "off",
        "max-len": "off",
        "padded-blocks": "off"
    }
}
